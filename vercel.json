{
	"version": 2,
	"builds": [
		{
			"src": "api/index.ts",
			"use": "@vercel/node",
			"config": {
				"tsConfig": "tsconfig.vercel.json"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/api/index.ts"
		}
	],
	"functions": {
		"api/index.ts": {
			"maxDuration": 30
		}
	},
	"env": {
		"NODE_ENV": "production"
	}
} 